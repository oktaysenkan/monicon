{
  "name": "@monicon/core",
  "version": "1.2.2",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*/index.cjs",
      "import": "./dist/*/index.mjs",
      "types": "./dist/*/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*/index.d.ts",
        "./dist/index.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.251",
    "@iconify/types": "^2.0.0",
    "@monicon/loader": "*",
    "@monicon/typescript-config": "*",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.14",
    "@types/pascalcase": "^1.0.3",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@iconify/utils": "^2.1.33",
    "change-case-all": "^2.1.0",
    "chokidar": "^4.0.3",
    "cosmiconfig": "^9.0.0",
    "eta": "^3.5.0",
    "fuuu": "^0.0.8",
    "glob": "^11.0.2",
    "html-to-jsx-transform": "^1.2.0",
    "jsdom": "^26.0.0",
    "lodash": "^4.17.21",
    "pascalcase": "^2.0.0",
    "prettier": "^3.5.3"
  },
  "peerDependencies": {
    "react-native-svg": ">=13.9.0"
  }
}
