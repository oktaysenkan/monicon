{
  "name": "@monicon/vite",
  "version": "2.0.5",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*/index.js",
      "import": "./dist/*/index.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "vite": "^4.0.0-0 || ^5.0.0-0 || ^6.0.0-0"
  },
  "devDependencies": {
    "@monicon/typescript-config": "2.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@monicon/core": "2.0.5"
  }
}
