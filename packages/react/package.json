{
  "name": "@oktaytest/react",
  "version": "0.0.122",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./iconify": {
      "import": "./dist/iconify.mjs",
      "require": "./dist/iconify.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepack": "yarn build"
  },
  "devDependencies": {
    "@oktaytest/typescript-config": "*",
    "@types/react": "^18.2.46",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@oktaytest/core": "*",
    "@oktaytest/icon-loader": "*"
  },
  "peerDependencies": {
    "react": ">=18.2.0"
  }
}
